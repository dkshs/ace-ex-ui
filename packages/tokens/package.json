{
  "name": "@ace-ex/tokens",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Visuais primitivos, como tipografia, cor e espaçamento, que são compartilhados entre plataformas.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "directory": "packages/tokens",
    "url": "git+https://github.com/ShadowsS01/ace-ex-ui.git"
  },
  "bugs": {
    "url": "https://github.com/ShadowsS01/ace-ex-ui/issues"
  },
  "homepage": "https://github.com/ShadowsS01/ace-ex-ui/tree/main/packages/tokens/README.md",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf .turbo"
  },
  "devDependencies": {
    "@ace-ex/ts-config": "*",
    "@ace-ex/eslint-config": "*",
    "eslint": "^8.30.0",
    "prettier": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "4.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
